{
    "env": {
        "browser": true,
        "es2021": true
    },
    "ignorePatterns": ["dist/*.js"],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "object-shorthand": "off",
      "no-underscore-dangle": "off",
      "func-names": "off"
    }
}
